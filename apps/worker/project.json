{
  "name": "worker",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/worker/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn tsc -b apps/worker/tsconfig.json"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn tsx apps/worker/src/client.ts"
      }
    }
  },
  "tags": []
}
