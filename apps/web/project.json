{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn workspace @formiq/web build"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn workspace @formiq/web dev -- --host 0.0.0.0 --port 4200"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "yarn eslint apps/web/src --ext .ts,.tsx"
      }
    }
  }
}
